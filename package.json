{
  "name": "twr-cli",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Dloong/twr-cli.git",
  "author": "shilong.duan@gmail.com",
  "license": "MIT",
  "scripts": {
      "serve": "cross-env-shell NODE_ENV=development ts-node --files -P ./build/tsconfig.json ./build/scripts/start.ts --open"
  },
  "bin": {
      "xr": "bin/twr",
      "xr-add": "bin/twr-add",
