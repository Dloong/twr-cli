      "xr-list": "bin/twr-list",
      "xr-init": "bin/twr-init"
  },
  "dependencies": {
      "chalk": "^4.1.0",
      "commander": "^6.1.0",
      "inquirer": "^7.3.3"
  },
  "devDependencies": {
      "husky": "^4.2.5"
  },
  "husky": {
      "hooks": {
          "commit-msg": "commitlint -c .commitlintrc.js -E HUSKY_GIT_PARAMS"
      }
  }
}
